<div class="">
  <div class="" style="padding: 50px 50px">
    <div class="flex justify-center">
      <div>
        <h1 class="text-6xl font-extrabold dark:text-gray-900 mb-7">
          OUR PROJECTS
        </h1>
      </div>
      <div class="relative top-2">
        <img src="/logo.png" class="w-20 ml-2" alt="ISSUE TRACKER" />
      </div>
    </div>

    <div class="grid grid-cols-3 gap-4 mb-4">
      <% projects.forEach(project => { %>
        <!-- <li><a href="/<%= project._id %>"><%= project.name %></a></li> -->
        <div class="flex items-center justify-center  rounded">
            <div
              class="p-5 bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700"
            >
                <h5
                  class="mb-2 text-xl font-semibold tracking-tight text-gray-900 dark:text-white"
                >
                  Project Name - <%= project.name %>
                </h5>
              <p class="mb-3 font-normal text-gray-500 dark:text-gray-400">
               <%= project.description %>
              </p>
  
              <div class="flex flex-col items-end">
                <h5
                  class="mb-1 text-xl font-medium text-gray-900 dark:text-white"
                >
                  <%= project.author %>
                </h5>
              </div>
              <hr class="h-px my-3 bg-gray-200 border-0 dark:bg-gray-700" />
  
              <div class="flex items-center justify-center gap-4">
                <button
                  onclick="window.location.href='/projects/<%= project._id %>'"
                  type="button"
                  class="text-white bg-blue-700 rounded-lg hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800hover:bg-gradient-to-bl focus:ring-4 focus:outline-none focus:ring-blue-300 dark:focus:ring-blue-800 font-medium rounded-lg text-sm px-5 py-2.5 text-center mr-2 transition ease-in-out delay-150 hover:-translate-x hover:scale-110 hover:bg-indigo-500 duration-300 ..."
                >
                  More <i class="fa-solid fa-arrow-right fa-sm"></i>
                </button>
                <button
                  onclick="window.location.href='/projects/delete/<%= project._id %>'"
                  type="button"
                  class="text-white bg-red-700 rounded-lg hover:bg-red-800 focus:ring-4 focus:outline-none focus:ring-red-300 dark:bg-red-600 dark:hover:bg-red-700 dark:focus:ring-red-800hover:bg-gradient-to-bl focus:ring-4 focus:outline-none focus:ring-red-300 dark:focus:ring-red-800 font-medium rounded-lg text-sm px-5 py-2.5 text-center mr-2 transition ease-in-out delay-150 hover:-translate-x hover:scale-110 hover:bg-indigo-500 duration-300 ..."
                >
                  Delete <i class="fa-regular fa-trash-can"></i>
                </button>
              </div>
            </div>
        </div>
      <% }); %>
    </div>
  </div>
</div>
